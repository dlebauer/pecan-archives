journal = {Nature},
keywords = {Biological,Biomass,Climate,Ecosystem,Geography,Models,Nutritional Physiological Phenomena,Photosynthesis,Plant Leaves,Plant Leaves: anatomy \& histology,Plant Leaves: chemistry,Plant Leaves: growth \& development,Plant Leaves: physiology,Rain},
