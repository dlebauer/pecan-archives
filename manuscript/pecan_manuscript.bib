issn = {1432-1939},
journal = {Oecologia},
keywords = {carbon cycle \'{a} data-model,eddy covariance \'{a} howland,forest \'{a} inverse modeling,fusion \'{a},parameter estimation \'{a} uncertainty,\'{a}},
